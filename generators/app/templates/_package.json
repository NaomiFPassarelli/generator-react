{
  "name": "<%= projectNameSlugified %>",
  "title": "<%= projectName %>",
  "description": "<%= projectDescription %>",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "<%= repoUrl %>"
  },
  "dependencies": {
    if (this.includeAphrodite && !this.includeRadium) {
    "aphrodite": "^1.2.3"<%
    }

    if (!this.includeAphrodite && this.includeRadium) {%>,
    "radium": "^0.19.4"<%
    }

    if (this.includeInext) {%>,
    "i18next": "^9.0.0"<%
    }

    if (this.includeMobileDetect) {%>,
    "mobile-detect": "^1.3.7"<%
    }

    if (this.includeMoment) {%>,
    "moment": "^2.18.1"<%
    }

    if (this.includeNukaCarousel) {%>,
    "nuka-carousel": "^2.3.0"<%
    }

    if (this.includeNumeral) {%>,
    "numeral": "^2.0.6"<%
    }

    if (this.includePostcss) {%>,
    "postcss": "^6.0.11"<%
    }

    if (this.includeReactAlert) {%>,
    "react-alert": "^2.3.0"<%
    }

    if (this.includeReactModal) {%>,
    "react-modal": "^2.3.2"<%
    }

    if (this.includeReactResponsive) {%>,
    "react-responsive": "^1.3.4"<%
    }

    if (this.includeReactScroll) {%>,
    "react-scroll": "^1.5.4"<%
    }

    if (this.includeReactShare) {%>,
    "react-share": "^1.16.0"<%
    }

    if (this.includeReactVirtualized) {%>,
    "react-virtualized": "^9.9.0"<%
    }

    if (this.includeRecharts) {%>,
    "recharts": "^1.0.0-alpha.4"<%
    }

    if (this.includeReactGoogleMaps) {%>,
    "react-google-maps": "^7.3.0"<%
    }

    if (this.includeReselect) {%>,
    "reselect": "^3.0.1"
    }
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.6.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "husky": "^0.14.3",
    "prettier": "^1.6.1",
    "prettier-eslint": "^8.0.0",
    "react-scripts": "^1.0.13"
  }
}
